/*
* %version: 12 %
*/

#include <ecom/registryinfo.rh>

RESOURCE REGISTRY_INFO theInfo
	{
	// UID for the DLL
	dll_uid = 0x10200ec9;
	// Declare array of interface info
	interfaces = 
		{
		INTERFACE_INFO
			{
			// UID of interface that is implemented
			interface_uid = 0x10206999;
			implementations = 
				{
				// Info for default impl.
				IMPLEMENTATION_INFO
					{
					implementation_uid = 0x1020699a;
					version_no         = 1;
					display_name       = "EAP over IKEv2";
					default_data       = "EAPVPNIF";
					opaque_data        = "";
					}
				};
			}
		};
	}

